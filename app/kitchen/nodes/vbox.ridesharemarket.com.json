{
  "name": "vbox",
  "set_fqdn": "vbox.ridesharemarket.com",
  "postfix": {
    "main": {
      "hostname": "vbox",
      "mydomain": "ridesharemarket.com",
      "smtpd_use_tls": "no"
    },
    "aliases": {
      "root": "systemsadmin@ridesharemarket.com"
    }
  },
  "users": [
    "rsm-data"
  ],
  "ssh-config": {
    "users": [
      "vagrant"
    ]
  },
  "tmux": {
    "users": [
      {
        "user": "vagrant",
        "home": "/home/vagrant"
      }
    ]
  },
  "run_list": [
    "recipe[network-hosts]",
    "recipe[common]",
    "recipe[firehol]",
    "recipe[docker-wrapper]",
    "recipe[docker-prometheus]",
    "recipe[docker-grafana]",
    "recipe[docker-relk]",
    "recipe[mongodb]",
    "recipe[git-repos]",
    "recipe[docker-containers]",
    "recipe[jenkins-cookbook]",
    "recipe[cron-jobs]"
  ]
}