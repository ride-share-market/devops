{
  "id": "prd_aws_ridesharemarket",
  "hosts": [
    {
      "id": "redline",
      "description": "NOC Server",
      "roles": [
        "noc01",
        "rmq01",
        "els01",
        "log01",
        "met01",
        "rpc01",
        "reg01",
        "mgo01",
        "www01",
        "api01"
      ],
      "cnames": [
        "noc.prd.ridesharemarket.com"
      ],
      "cloud": {
        "id": "6084027",
        "ip": {
          "eth0": "188.166.127.172",
          "eth1": "10.133.255.122"
        },
        "size": "4gb"
      },
      "chefBootstrap": {
        "runList": [
          "recipe[firehol-prd]",
          "recipe[network-hosts-prd]",
          "recipe[common]",
          "recipe[chef-client]",
          "recipe[chef-client::delete_validation]"
        ],
        "jsonAttributes": {
          "set_fqdn": "redline.ridesharemarket.com",
          "postfix": {
            "main": {
              "hostname": "redline",
              "mydomain": "ridesharemarket.com",
              "smtpd_use_tls": "no"
            },
            "aliases": {
              "root": "systemsadmin@ridesharemarket.com"
            }
          }
        }
      }
    }
  ]
}