{
  "id": "prd_aws_ridesharemarket",
  "hosts": [
    {
      "id": "trumpet",
      "description": "Database, ELK, CI, StatsD, Prometheus server",
      "roles": [
        "noc01",
        "rmq01",
        "els01",
        "log01",
        "met01",
        "rpc01",
        "reg01",
        "mgo01",
        "cch01"
      ],
      "cnames": [
        "noc.prd.ridesharemarket.com"
      ],
      "cloud": {
        "id": "xxxxxx",
        "ip": {
          "eth0": "188.166.127.172",
          "eth1": "10.133.255.122"
        }
      },
      "chefBootstrap": {
        "runList": [
          "recipe[firehol-prd]",
          "recipe[network-hosts-prd]",
          "recipe[common]",
          "recipe[chef-client]",
          "recipe[chef-client::delete_validation]"
        ],
        "jsonAttributes": {
          "set_fqdn": "trumpet.ridesharemarket.com",
          "postfix": {
            "main": {
              "hostname": "trumpet",
              "mydomain": "ridesharemarket.com",
              "smtpd_use_tls": "no"
            },
            "aliases": {
              "root": "systemsadmin@ridesharemarket.com"
            }
          }
        }
      }
    },
    {
      "id": "mandolin",
      "description": "Web API and Web App server",
      "roles": [
        "www01",
        "api01"
      ],
      "cloud": {
        "id": "i-37b06693",
        "ip": {
          "eth0": "52.76.79.143",
          "eth1": "10.0.0.30"
        }
      },
      "chefBootstrap": {
        "runList": [
          "recipe[network-hosts-prd]",
          "recipe[common]",
          "recipe[chef-client]",
          "recipe[chef-client::delete_validation]"
        ],
        "jsonAttributes": {
          "set_fqdn": "mandolin.ridesharemarket.com",
          "postfix": {
            "main": {
              "hostname": "mandolin",
              "mydomain": "ridesharemarket.com",
              "smtpd_use_tls": "no"
            },
            "aliases": {
              "root": "systemsadmin@ridesharemarket.com"
            }
          }
        }
      }
    }
  ]
}